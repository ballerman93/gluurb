local function promptNotification()
    local screenGui = Instance.new("ScreenGui") -- tip: dont change this to something else thinking this is the name of the ScreenGui like ofc it cant find a fucking instance named E
    screenGui.Name = "ClientNotificationGUI"
    screenGui.ResetOnSpawn = false
    screenGui.IgnoreGuiInset = true
    screenGui.Parent = player:WaitForChild("PlayerGui")

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 300, 0, 100)
    frame.AnchorPoint = Vector2.new(0.5, 0.5)
    frame.Position = UDim2.new(0.5, 0, 0.5, 0) -- fuck you asshole
    frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    frame.BorderSizePixel = 0
    frame.Parent = screenGui

    local titleBox = Instance.new("TextBox")
    titleBox.PlaceholderText = "Enter Text"
    titleBox.Size = UDim2.new(1, -20, 0, 40)
    titleBox.Position = UDim2.new(0, 10, 0, 10)
    titleBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    titleBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    titleBox.TextScaled = true
    titleBox.BorderSizePixel = 0
    titleBox.Parent = frame

    local sendButton = Instance.new("TextButton")
    sendButton.Text = "gloop"
    sendButton.Size = UDim2.new(1, -20, 0, 40)
    sendButton.Position = UDim2.new(0, 10, 0, 55)
    sendButton.BackgroundColor3 = Color3.fromRGB(70, 130, 180)
    sendButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    sendButton.TextScaled = true
    sendButton.BorderSizePixel = 0
    sendButton.Parent = frame

    sendButton.MouseButton1Click:Connect(function()
        local title = titleBox.Text
        if title ~= "" then
            notifEvent:FireServer(title)
            screenGui:Destroy()
        end
    end)
end
