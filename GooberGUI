    title.Size = UDim2.new(1, -40, 0, 40)
    title.Position = UDim2.new(0, 10, 0, 0)
    title.BackgroundTransparency = 0.3
    title.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    title.BorderSizePixel = 0
    title.Text = "Play Dump after you select one of these"
    title.TextColor3 = Color3.new(1, 1, 1)
    title.Font = Enum.Font.GothamBold
    title.TextScaled = true
    title.Parent = frame
    Instance.new("UICorner", title).CornerRadius = UDim.new(0, 8)

    local closeBtn = Instance.new("TextButton")
    closeBtn.Size = UDim2.new(0, 20, 0, 20)
    closeBtn.Position = UDim2.new(1, -25, 0, 10)
    closeBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
    closeBtn.TextColor3 = Color3.new(1, 1, 1) --minesweeper shit agian
    closeBtn.Text = "X"
    closeBtn.Parent = frame
    Instance.new("UICorner", closeBtn).CornerRadius = UDim.new(0, 6)

    closeBtn.MouseButton1Click:Connect(function()
        screenGui:Destroy()
    end)

    local folderNames = {
        "Field of Hopes and Dreams [Admin owo]", -- kill me
        "Sweet But Psycho [ABMIN]",
        "still into you [abmin uwuu nyaasaaaaa]", -- kill me
        "Collapse of Ego (psst admin)"
        -- why did i add fucking vapor buster its literally in the game am i schizo
    }

    local buttons = {}
    for i, name in ipairs(folderNames) do
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(1, -20, 0, 40)
        btn.Position = UDim2.new(0, 10, 0, 50 + (i-1)*50)
        btn.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
        btn.TextColor3 = Color3.new(1, 1, 1)
        btn.Text = name
        btn.Font = Enum.Font.GothamBold
        btn.TextScaled = true
        btn.Parent = frame
        Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)
        table.insert(buttons, btn)

        btn.MouseButton1Click:Connect(function()
            for _, child in ipairs(dumpFolder:GetChildren()) do
                child:Destroy()
            end

            local sourceFolder = songsFolder:FindFirstChild(name)
            if sourceFolder then
                for _, item in ipairs(sourceFolder:GetChildren()) do
                    item:Clone().Parent = dumpFolder
                end
            end

            local dumpAudio = musicFolder:FindFirstChild("Dump")
            if dumpAudio then
                dumpAudio:Destroy()
            end

            local adminAudio = musicFolder:FindFirstChild(name)
            if adminAudio then
                local newDump = adminAudio:Clone()
                newDump.Name = "Dump"
                newDump.Parent = musicFolder
            end
            screenGui:Destroy()
        end)
    end
end)

