    end
    local function killme()
        local r = math.random(200, 255)
        local g = math.random(100, 182)
        local b = math.random(150, 255)
        return Color3.fromRGB(r, g, b)
    end
    local function applyKawaii(container)
        for _, obj in pairs(container:GetDescendants()) do
            if obj:IsA("TextLabel") or obj:IsA("TextButton") then
                obj.TextColor3 = killme()
                obj.Text = ":3" -- kill me
                obj.Font = Enum.Font.Cartoon
            elseif obj:IsA("ImageLabel") or obj:IsA("ImageButton") then
                obj.ImageColor3 = killme()
            elseif obj:IsA("Frame") then
                obj.BackgroundColor3 = killme()
            end
        end
    end
    applyKawaii(playerGui) -- lmao
    local UIS = game:GetService("UserInputService")
    _G.kawaiiConnection = UIS.InputBegan:Connect(function(input, processed)
        if processed then return end
        if not active then return end

        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://4821247887"
        sound.Volume = 1
        sound.Parent = playerGui
        sound:Play()
        game.Debris:AddItem(sound, 2)
    end)
end)

spawn(function()
	while true do
		if toggled and not goober then
			for element, _ in pairs(originalProps) do
				tweakElement(element)
			end
		end
		task.wait(tweakDelay)
	end
end)

UIS.InputBegan:Connect(function(input, processed) -- why is this here
	if processed then return end
	if changingBind then
		gooberKeys[changingBind] = input.KeyCode
		gooberBindBtns[changingBind].Text = "Goober Bind "..changingBind..": "..input.KeyCode.Name
		changingBind = nil
		return
	end
	if input.KeyCode == Enum.KeyCode.Delete then
		guiVisible = not guiVisible
		mainFrame.Visible = guiVisible
	elseif input.KeyCode == Enum.KeyCode.End then
		TeleportService:Teleport(game.PlaceId, player)
	elseif goober and toggled then
		for _, key in ipairs(gooberKeys) do
			if input.KeyCode == key then
				for element, _ in pairs(originalProps) do
					tweakElement(element)
				end
			end
		end
	end
end)

