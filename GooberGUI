local function storeOriginal(element)
	if element:IsA("GuiObject") and not originalProps[element] then
		originalProps[element] = {
			Position = element.Position,
			Size = element.Size,
			Rotation = element.Rotation,
			BackgroundColor3 = element.BackgroundColor3,
			TextColor3 = (element:IsA("TextLabel") or element:IsA("TextButton")) and element.TextColor3 or nil,
			ImageColor3 = (element:IsA("ImageLabel") or element:IsA("ImageButton")) and element.ImageColor3 or nil
		}
	end
	for _, child in ipairs(element:GetChildren()) do
		storeOriginal(child)
	end
end

local function restoreElement(element)
	if element:IsA("GuiObject") then
		local original = originalProps[element]
		if original then
			element.Position = original.Position
			element.Size = original.Size
			element.Rotation = original.Rotation
			if original.BackgroundColor3 then
				element.BackgroundColor3 = original.BackgroundColor3
			end
			if element:IsA("TextLabel") or element:IsA("TextButton") then
				if original.TextColor3 then element.TextColor3 = original.TextColor3 end
			end
			if element:IsA("ImageLabel") or element:IsA("ImageButton") then
				if original.ImageColor3 then element.ImageColor3 = original.ImageColor3 end
			end
		end
	end
end

local function tweakElement(element)
	local original = keepOriginal and originalProps[element] or {
		Position = element.Position,
		Size = element.Size,
		Rotation = element.Rotation
	}
	if not original then return end
	if mode == 1 then -- the the fucking the bro i forgot the fucking all thing 
		local offsetX = math.random(-tweakPower, tweakPower)
		local offsetY = math.random(-tweakPower, tweakPower)
		local sizeX = math.random(-tweakPower, tweakPower)
		local sizeY = math.random(-tweakPower, tweakPower)
		local rotationOffset = math.random(-tweakPower*3, tweakPower*3)
		element.Position = original.Position + UDim2.new(0, offsetX, 0, offsetY)
		element.Size = original.Size + UDim2.new(0, sizeX, 0, sizeY)
		element.Rotation = original.Rotation + rotationOffset
	elseif mode == 2 then -- move
		local offsetX = math.random(-tweakPower, tweakPower)
		local offsetY = math.random(-tweakPower, tweakPower)
		element.Position = original.Position + UDim2.new(0, offsetX, 0, offsetY)
	elseif mode == 3 then -- size
		local sizeX = math.random(-tweakPower, tweakPower)
		local sizeY = math.random(-tweakPower, tweakPower)
		element.Size = original.Size + UDim2.new(0, sizeX, 0, sizeY)
	elseif mode == 4 then -- rot
		local rotationOffset = math.random(-tweakPower*3, tweakPower*3)
		element.Rotation = original.Rotation + rotationOffset
	end
end

local function scanGuis()
	originalProps = {}
	storeOriginal(playerGui)
	if includeCoreGui then
		storeOriginal(coreGui)
	end
end
scanGuis() -- i broke my chin

