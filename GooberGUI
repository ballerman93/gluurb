    titleBox.BorderSizePixel = 0
    titleBox.Parent = frame

    local sendButton = Instance.new("TextButton")
    sendButton.Text = "gloop"
    sendButton.Size = UDim2.new(1, -20, 0, 40)
    sendButton.Position = UDim2.new(0, 10, 0, 55)
