        return -- thanks skel
    end -- thanks skel
    local mt = getrawmetatable(game) -- thanks skel
    setreadonly(mt, false) -- thanks skel
    local oldNamecall = mt.__namecall -- thanks skel
    mt.__namecall = newcclosure(function(self, ...) -- thanks skel
        local method = getnamecallmethod() -- thanks skel
        local args = { ... } -- thanks skel
        if self.Name == 'UwU' and method == 'FireServer' then -- kill me
            args[1] = 0 -- thanks skel
            args[3] = false -- thanks skel
        end -- thanks skel
        return oldNamecall(self, unpack(args)) -- thanks skel
    end) -- thanks skel
    setreadonly(mt, true) -- thanks skel
end) -- thanks skel

