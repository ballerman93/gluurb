        _G.sizererSmoothness = 0
    end
    b.Text = string.format("Smoothness: %.2f", _G.sizererSmoothness) -- fuck luau
end)

local mentalretardationmode = addButton("Confusion Mode: false", function(b) -- crack smoking
    confusionActive = not confusionActive
    b.Text = "Confusion Mode: "..tostring(confusionActive)
    if not confusionActive then
        notesContainer.Size = _G.confusionBaseSize or notesContainer.Size
        return
    end
    local baseSize = notesContainer.Size
    _G.confusionBaseSize = baseSize -- where is the part where my sanity gets restored
    local targetUp = UDim2.new(baseSize.X.Scale, baseSize.X.Offset, baseSize.Y.Scale + 0.5, baseSize.Y.Offset)
    local targetDown = UDim2.new(baseSize.X.Scale, baseSize.X.Offset, baseSize.Y.Scale - 0.5, baseSize.Y.Offset)
    task.spawn(function()
        while confusionActive do
            local tweenUp = tweenService:Create(notesContainer, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Size = targetUp})
            tweenUp:Play()
            tweenUp.Completed:Wait()
            if not confusionActive then break end
