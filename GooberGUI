local beatBindBtns = {}
for i = 1, 4 do
    beatBindBtns[i] = addButton("Beat Bind "..i..": "..beatKeys[i].Name, function(b)
        changingBeatBind = i
        b.Text = "Press any key..."
    end)
end

UIS.InputBegan:Connect(function(input, gameProcessed)
    if changingBeatBind then
        beatKeys[changingBeatBind] = input.KeyCode
        beatBindBtns[changingBeatBind].Text = "Beat Bind "..changingBeatBind..": "..input.KeyCode.Name
        changingBeatBind = nil
        return
    end
    if beatMode and not gameProcessed then
        local targetOffsetX = nil
        if input.KeyCode == beatKeys[1] or input.KeyCode == beatKeys[2] then
            targetOffsetX = -beatDistance
        elseif input.KeyCode == beatKeys[3] or input.KeyCode == beatKeys[4] then
            targetOffsetX = beatDistance
        end
        if targetOffsetX then
            youFrame:TweenPosition(
                UDim2.new(idiotPos.X.Scale, idiotPos.X.Offset + targetOffsetX,
                          idiotPos.Y.Scale, idiotPos.Y.Offset),
                "Out", "Linear", beatSpeedOut, true,
                function()
                    youFrame:TweenPosition(
                        idiotPos,
                        "Out", "Quad", beatSpeedBack, true -- ??
                    )
                end
            )
        end
    end
end)

local sizerBtn = addButton("Sizer Mode: false", function(b) -- this is pretty cool actually -- nevermind i just added sizerer and this is fucking garbage
    local active = not (_G.sizerActive or false)
    _G.sizerActive = active
    b.Text = "Sizer Mode: "..tostring(active)
    if not active then
        if _G.sizerTween then
            _G.sizerTween:Cancel()
            _G.sizerTween = nil
        end
        youFrame.Size = _G.sizerBaseSize or youFrame.Size
        return
    end
    _G.sizerBaseSize = _G.sizerBaseSize or youFrame.Size
    local baseSize = _G.sizerBaseSize
    task.spawn(function()
        while _G.sizerActive do
            local targetUp = UDim2.new(baseSize.X.Scale + 0.2, baseSize.X.Offset, baseSize.Y.Scale + 0.2, baseSize.Y.Offset)
            local targetDown = UDim2.new(baseSize.X.Scale - 0.2, baseSize.X.Offset, baseSize.Y.Scale - 0.2, baseSize.Y.Offset)
            local tweenUp = tweenService:Create(youFrame, TweenInfo.new(0.365, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Size = targetUp}) -- i have spent like 5 minutes trying to get it to sync right to that 5am song
            tweenUp:Play()
            _G.sizerTween = tweenUp
            tweenUp.Completed:Wait()
            if not _G.sizerActive then break end
            local tweenDown = tweenService:Create(youFrame, TweenInfo.new(0.165, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Size = targetDown}) -- mofoka put 0.525 ainnoway
            tweenDown:Play()
            _G.sizerTween = tweenDown
            tweenDown.Completed:Wait()
        end
        youFrame.Size = baseSize
    end)
end)

local sizererBtn = addButton("Sizerer Mode: false", function(b) -- i love my life i love this i love myself im gonna hug myself this is the best thing in my entire life
    local active = not (_G.sizererActive or false)
    _G.sizererActive = active
    b.Text = "Sizerer Mode: "..tostring(active)
    if not active then
        _G.sizererActive = false
        youFrame.Size = _G.sizererBaseSize or youFrame.Size
        return
    end
    _G.sizererBaseSize = _G.sizererBaseSize or youFrame.Size
    local baseSize = _G.sizererBaseSize
    local soundContainers = {workspace, game:GetService("ReplicatedStorage")}
    local cachedSounds = {}
    for _, container in ipairs(soundContainers) do
        for _, obj in ipairs(container:GetDescendants()) do
            if obj:IsA("Sound") then
                table.insert(cachedSounds, obj)
            end
        end
    end
    _G.sizererSmoothness = _G.sizererSmoothness or 0.2 -- existence
    task.spawn(function()
        local currentScale = 1
        while _G.sizererActive do
            local totalLoudness = 0
            local soundCount = 0
            for _, sound in ipairs(cachedSounds) do --c ac
                if sound.IsPlaying then
                    totalLoudness = totalLoudness + sound.PlaybackLoudness
                    soundCount = soundCount + 1
                end
            end
            local targetScale = 1
            if soundCount > 0 then
                local averageLoudness = totalLoudness / soundCount
                targetScale = 1 + (averageLoudness / 125) -- the the scale the fuck okayayayayaayaya8y8sdsyufdsfiuhponij ass
            end
            local smoothness = _G.sizererSmoothness or 0.2
            currentScale = currentScale + (targetScale - currentScale) * (1 - smoothness)
            local targetSize = UDim2.new(baseSize.X.Scale * currentScale, baseSize.X.Offset,
                                         baseSize.Y.Scale * currentScale, baseSize.Y.Offset)
            youFrame.Size = targetSize
            RunService.Heartbeat:Wait()
        end
        youFrame.Size = baseSize
    end)
end)

