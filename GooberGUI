local boo = addButton("testering: false", function(b)
    local active = not (_G.freeCamActive or false)
    _G.freeCamActive = active -- original freecam but inf yeild so no fuck ofcf nolt chanigng the fustupid vars
    b.Text = "testering: "..tostring(active)
    if active then
        _G.originalCameraType = camera.CameraType
        camera.CameraType = Enum.CameraType.Scriptable
        camera.CFrame = player.Character and player.Character:FindFirstChild("HumanoidRootPart") 
            and CFrame.new(player.Character.HumanoidRootPart.Position) 
            or camera.CFrame
        local sensitivity = 1
        local yaw = 0
        local pitch = 0
        local lastMousePos
        _G.freeCamInputConn = UserInputService.InputChanged:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseMovement and _G.freeCamActive then
                if lastMousePos then
                    local delta = input.Position - lastMousePos
                    yaw = yaw - delta.X * sensitivity * 0.01
                    pitch = math.clamp(pitch - delta.Y * sensitivity * 0.01, -math.pi, math.pi)
                end
                lastMousePos = input.Position
            end
        end)
        UserInputService.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseMovement and _G.freeCamActive then
                lastMousePos = nil
            end
        end)
        _G.freeCamRenderConn = RunService.RenderStepped:Connect(function()
            if not _G.freeCamActive then return end
            local charHRP = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            local pos = charHRP and charHRP.Position or camera.CFrame.Position
            local rot = CFrame.Angles(pitch, yaw, 0)
            camera.CFrame = CFrame.new(pos) * rot
        end)
    else
        if _G.freeCamInputConn then
            _G.freeCamInputConn:Disconnect()
            _G.freeCamInputConn = nil
        end
        if _G.freeCamRenderConn then
            _G.freeCamRenderConn:Disconnect()
            _G.freeCamRenderConn = nil
        end
        camera.CameraType = _G.originalCameraType or Enum.CameraType.Custom
    end
end)

