spawn(function()
	while true do
		if toggled and not goober then
			for element, _ in pairs(originalProps) do
				tweakElement(element)
			end
		end
		task.wait(tweakDelay)
	end
end)

UIS.InputBegan:Connect(function(input, processed) -- why is this here
	if processed then return end
	if changingBind then
		gooberKeys[changingBind] = input.KeyCode
		gooberBindBtns[changingBind].Text = "Goober Bind "..changingBind..": "..input.KeyCode.Name
		changingBind = nil
		return
	end
	if input.KeyCode == Enum.KeyCode.Delete then
		guiVisible = not guiVisible
		mainFrame.Visible = guiVisible
		local sound = Instance.new("Sound") -- cant decide between sound or sfx
		sound.Name = "harharharharhar"
		sound.SoundId = "rbxassetid://8930408685"
		sound.Volume = 1
		sound.Parent = coreGui
		sound:Play()
		task.spawn(function()
			sound.Ended:Wait()
			sound:Destroy()
		end)
	elseif input.KeyCode == Enum.KeyCode.End then
		TeleportService:Teleport(game.PlaceId, player)
	elseif goober and toggled then
		for _, key in ipairs(gooberKeys) do
			if input.KeyCode == key then
				for element, _ in pairs(originalProps) do
					tweakElement(element)
				end
			end
		end
	end
end)

