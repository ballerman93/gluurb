local circleBtn = addButton("Circle Mode: "..tostring(circleMode), function(b) -- actually fun
    circleMode = not circleMode
    b.Text = "Circle Mode: "..tostring(circleMode)
    if youFrame then
        local idiotPos = youFrame.Position
        task.spawn(function()
            local angle = 0
            while circleMode do
                angle = angle + circleSpeed * task.wait() * math.pi * 2
                local offsetX = math.cos(angle) * circleRadius
                local offsetY = math.sin(angle) * circleRadius -- stand on the eon
                youFrame.Position = UDim2.new(
                    idiotPos.X.Scale,
                    idiotPos.X.Offset + offsetX,
                    idiotPos.Y.Scale,
                    idiotPos.Y.Offset + offsetY
                )
            end
            youFrame.Position = idiotPos
        end)
    end
end)

